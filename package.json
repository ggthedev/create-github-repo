{
  "name": "create-github-repo",
  "version": "0.1.2",
  "description": "Create your github repos like a champ!",
  "main": "lib/index.js",
  "bin": "lib/index.js",
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "",
    "babel:transpile": "babel --presets es2015 ./src --out-dir ./lib",
