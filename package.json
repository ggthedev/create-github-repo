  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-eslint": "6.0.2",
    "babel-preset-es2015": "6.6.0",
    "eslint": "2.7.0",
    "eslint-config-airbnb": "6.2.0",
    "eslint-plugin-react": "4.2.3"
  },
  "dependencies": {
    "bluebird": "3.3.4",
    "chalk": "1.1.3",
    "commander": "2.9.0",
    "github": "0.2.4",
    "promisify-node": "0.4.0",
    "random-item": "1.0.0"
